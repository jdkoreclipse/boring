mount("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/cache", "/cache");
delete_recursive("/cache");
ui_print(" ");
ui_print(" ");
ui_print("Formatting system...");
unmount("/system");
format("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/system");
mount("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/system", "/system");
delete_recursive("/system");
ui_print(" ");
ui_print(" ");
ui_print("Formatting data...");
mount("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/userdata", "/data");
delete_recursive("/data/data");
delete_recursive("/data/system");
delete_recursive("/data/dalvik-cache");
delete_recursive("/data/app");
delete_recursive("/data/app-private");
delete_recursive("/data/user");
run_program("/sbin/umount", "-l", "/cache");
run_program("/sbin/mount", "-o ro", "/cache");
assert(unmount("/cache") || ui_print("(cache is unmounted already)"));
format("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/cache");
assert(unmount("/system") || ui_print("(system is unmounted already)"));
format("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/system");
mount("ext4", "EMMC", "/dev/block/platform/omap/omap_hsmmc.0/by-name/system", "/system");
run_program("/sbin/busybox", "mount", "/data");
ui_print("Flashing system...");
package_extract_dir("system", "/system");
ui_print("Flashing data...");
package_extract_dir("data", "/data");
ui_print("Writing symlinks...");
